<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="proxima.informatica.academy.dto.CandidateStatesDto"
		table="candidate_states">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="role_id"></property>
		<property name="status_code"></property>
		<property name="status_label"></property>
		<property name="status_description"></property>
		<property name="status_color"></property>

	</class>

</hibernate-mapping>