<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="proxima.informatica.academy.dto.SurveyrepliesDto" table="surveyreplies">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="survey_id"></property>
		<property name="user_id"></property>
		<property name="starttime"></property>
        <property name="endtime"></property>
		<property name="answers"></property>
		<property name="pdffilename"></property>
		<property name="points"></property>
	</class>

</hibernate-mapping>