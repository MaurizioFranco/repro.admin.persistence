<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="proxima.informatica.academy.dto.UserDto" table="users">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="email"></property>
		<property name="password"></property>
		<property name="firstname"></property>
        <property name="lastname"></property>
		<property name="dateofbirth"></property>
		<property name="regdate"></property>
		<property name="role"></property>
		<property name="imgpath"></property>
		<property name="note"></property>
		<property name="enabled"></property>
	</class>

</hibernate-mapping>